{
  "name": "yapi-ts-codegen",
  "version": "1.0.2",
  "description": "A tool to generate typescript interfaces from yapi",
  "main": "lib/index.js",
  "bin": {
    "yts": "lib/cli.js"
  },
  "types": "lib/index.d.ts",
  "keywords": [
    "yapi",
    "codegen",
    "typescript"
  ],
  "homepage": "https://github.com/brucecham/yapi-ts-codegen",
  "bugs": {
    "url": "https://github.com/brucecham/yapi-ts-codegen/issues"
  },
  "author": {
    "name": "Bruce Cham",
    "email": "brucecham2024@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brucecham/yapi-ts-codegen.git"
  },
  "files": [
    "lib"
  ],
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "build": "tsc",
    "start": "ts-node --esm src/index.ts",
    "prepare": "npm run build",
    "ci": "npm run build && npm run check-format && npm run check-exports",
    "test": "vitest run",
    "dev": "npm run build && node ./lib/index.js"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.2",
    "@types/node": "^22.10.2",
    "prettier": "^3.4.2",
    "request": "^2.88.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@types/commander": "^2.12.0",
    "@types/fs-extra": "^11.0.4",
    "@types/request": "^2.48.12",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "quicktype-core": "^23.0.170",
    "tslib": "^2.8.1"
  }
}
